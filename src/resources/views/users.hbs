<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Permission</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
        <tr>
            <th scope="row">{{sum @index 1}}</th>
            <td>{{this.username}}</td>
            <td>{{this.quyenhan}}</td>
            <td>{{this.email}}</td>
        </tr>
    {{/each}}
  </tbody>
</table>
